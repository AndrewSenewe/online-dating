<!DOCTYPE html>
 <html>
   <head>
     <!--Import Google Icon Font-->
     <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
     <!--Import materialize.css-->
     <link type="text/css" rel="stylesheet" href="css/materialize.min.css"  media="screen,projection"/>
     <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
     <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/css/materialize.min.css">
   </head>

   <body>

     <header>
       <nav>
        <div class="nav-wrapper">
          <a href="#" class="brand-logo">Welcome User</a>
          <ul id="nav-mobile" class="right hide-on-med-and-down">
            <li><a href="/logout">Logout</a></li>
          </ul>
        </div>
      </nav>
     </header>
     <br><br>

     <!-- <h4 align="center"><u>Restaurant List</u></h4> -->
     <br>
     <div class="row">
       <div class="col s4">
         <form action="#" method="post">
           <div class="row">
            <div class="col s12 m10">
              <div class="card">
                <div class="card-image">
                  <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/a/ac/No_image_available.svg/1024px-No_image_available.svg.png">
                </div>
                <!-- <div class="card-content"> -->
                  <div class="collection">
                   < <a class="collection-item"><span class="badge"><%=user.name%></span>Name</a>
                    <a class="collection-item"><span class="badge"><%=user.gender%></span>Gender</a>
                    <a class="collection-item"><span class="badge"><%=user.birthday.toDateString().slice(3)%></span>Birthday</a>
                    <a class="collection-item"><span class="badge"><%=user.email%></span>Email</a>
                    <a class="collection-item"><span class="badge"><%=user.phone%></span>Phone</a>
                 </div>
                <!-- </div> -->
                <div class="card-action">
                  <button type="submit" class="waves-effect waves-light btn" href="#">Update Profile</button>
                </div>
              </div>
            </div>
          </div>
         </form>
       </div>
       <div class="col s8">
         <h4>List of Mate</h4>
         <br>
         <% female.forEach(d => { %>
           <%if (d.Users.length !== 0) {%>
         <form class="" action="/mating/<%= d.Users[0].id%>_<%= d.Users[0].User_restaurant.createdAt.getTime()%>_<%= d.Users[0].User_restaurant.restaurantId%>" method="post">
             <div class="row">
               <ul class="collapsible" data-collapsible="accordion">
                 <li>
                   <div class="collapsible-header"><i class="material-icons">account_circle</i><%= d.Users[0].name%></div>
                   <div class="collapsible-body">
                     <div class="collection">
                       <a class="collection-item"><span class="badge"><%= d.Users[0].birthday.toDateString().slice(3)%></span>Birthday</a>
                     </div>
                     <button type="submit" class="waves-effect waves-light btn">select</button>
                   </div>
                 </li>
               </ul>
             </div>
           <%}%>
          <% }) %>
         </form>
       </div>
     </div>


     <footer style="background-color:#ee6e73; padding: 20px 20px;">
      <div class="footer-copyright">
         <div class="container">
           Powered By Ade | Andrew Â© 2017
         </div>
      </div>
     </footer>


     <!--Import jQuery before materialize.js-->
     <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
     <script type="text/javascript" src="js/materialize.min.js"></script>
     <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/js/materialize.min.js"></script>
     <script type="text/javascript">
     $(document).ready(function(){
       $('.materialboxed').materialbox();
      });
     </script>
   </body>

 </html>